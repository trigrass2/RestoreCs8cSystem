<?xml version="1.0" encoding="iso-8859-1"?>
<programList xmlns="ProgramNameSpace">
  <program name="listcursor1" public="false">
    <description />
    <paramSection>
      <param name="pView" type="num" byVal="true" />
      <param name="pShow" type="bool" byVal="true" />
    </paramSection>
    <localSection>
      <local name="lX" type="num" size="1" />
      <local name="lY" type="num" size="1" />
    </localSection>
    <source>
      <code>begin
  //
  // Staubli Copyright 2003 06 20
  //
  view=pView
  if (pShow==true)
    lX=topx+1
    lY=topy+1+pView
    gotoxy(lX,lY)
    put("þ")
    lX=topx+bottomx
    gotoxy(lX,lY)
    put("þ")
  else
    lX=topx+1
    lY=topy+1+pView
    gotoxy(lX,lY)
    put(" ")
    lX=topx+bottomx
    gotoxy(lX,lY)
    put(" ")
  endIf
end
            </code>
    </source>
  </program>
</programList>